{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "interface-name": [
      true,
      "always-prefix"
    ],
    "max-line-length": [
      true,
      140
    ],
    "member-ordering": [
      true,
      "public-before-private",
      "static-before-instance",
      "variables-before-functions"
    ],
    "object-literal-sort-keys": false,
    "trailing-comma": false
  }
}